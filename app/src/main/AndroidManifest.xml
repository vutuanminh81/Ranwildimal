<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ranwildimal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="androidx.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="RANWILDIMAL"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ranwildimal">
        <activity android:name=".ResultErrorActivity"></activity>
        <activity android:name=".ResultSuccessActivity" />
        <activity android:name=".DescriptionExempleListActivity" />
        <activity android:name=".DescriptionActivity" />
        <activity android:name=".SearchHistoryListActivity" />
        <activity android:name=".Encounter_List_Activity" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".AbouUsActivity" />
        <activity android:name=".MyEncounterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>